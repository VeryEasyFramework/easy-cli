{
  "name": "@vef/easy-cli",
  "version": "0.2.7",
  "exports": "./mod.ts",
  "imports": {
    "#/": "./src/",
    "@vef/string-utils": "jsr:@vef/string-utils@^0.1.5"
  },
  "publish": {
    "exclude": [
      "dev",
      ".github",
      ".vscode"
    ]
  },
  "lint": {
    "rules": {
      "exclude": ["no-unused-vars", "no-explicit-any"]
    }
  },
  "tasks": {
    "dev": "cd dev && deno run -A main.ts",
    "check": "deno publish --dry-run",
    "update": "deno add @vef/string-utils"
  }
}
